{
  "extends": "@react-native/typescript-config",
  "include": ["**/*.js", "**/*.jsx", "**/*.tsx", "**/*.tsx"],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app-screen/*": ["./src/screen/*"],
      "@app-component/*": ["./src/public/component/*"]
    }
  },
  "exclude": ["node_modules", "android", "ios"]
}
